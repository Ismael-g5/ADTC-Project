{#
/**
 * @file
 * Default theme implementation for a 'form' element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}
<form{{ attributes.addClass('search-form', 'search-block-form', 'form-row') }}>
  <input class="search-field" type="text" id="search" placeholder="Pesquisar">
  <button id="searchBtn" type="submit" class="btn-search"><img src="themes/custom/adtc/assets/images/search.svg"></button>
</form>
